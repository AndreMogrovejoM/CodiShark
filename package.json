{
  "name": "codisharkcollectionsystem",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "vite",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:all": "vitest run --coverage",
    "test:ci": "cross-env CI=true yarn test",
    "check-types": "tsc",
    "commit": "git-cz",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern !cypress/.eslintrc.js --ext .js,jsx,.ts,.tsx . --fix",
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx|json|css|scss|html)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "build": "vite build",
    "preview": "vite preview",
    "validate": "cross-env CI=true npm-run-all --parallel test check-types check-format lint build",
    "validate:ci": "npm-run-all --parallel check-types check-format lint",
    "deploy": "gh-pages -d build",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.11",
    "add": "^2.0.6",
    "antd": "^4.23.6",
    "axios": "^0.27.2",
    "dayjs": "1.11.4",
    "gh-pages": "^4.0.0",
    "history": "^5.3.0",
    "i18next": "^21.10.0",
    "i18next-browser-languagedetector": "^6.1.8",
    "intl": "^1.2.5",
    "react": "^18.2.0",
    "react-context-devtool": "^2.0.3",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.38.0",
    "react-i18next": "^11.18.6",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.4.2",
    "react-use-cookie": "^1.4.0",
    "styled-components": "^5.3.6",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^28.1.6",
    "@types/jest-in-case": "^1.0.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-react": "^2.1.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "c8": "^7.12.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-universe": "^11.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.3",
    "jest-axe": "^6.0.0",
    "jest-environment-jsdom": "^28.1.3",
    "jest-extended": "^3.0.1",
    "jest-in-case": "^1.0.2",
    "jest-watch-typeahead": "^2.0.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.7",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-svgr": "^2.1.0",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.12.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.types.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "lines": 0,
        "functions": 0
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
